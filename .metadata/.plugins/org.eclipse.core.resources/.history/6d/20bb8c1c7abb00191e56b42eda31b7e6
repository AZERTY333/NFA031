package net.pg3da97n.Apprentissage;

public class Les_boucles {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//La boucle for
		int note = 12;
		for(int i=0;i<=12;i++)
			System.out.println("La note est de :" + i);
		//boucle for infinie
		for(;;) {
			System.out.println("La note est de :" + note);
		int i = 0;
		i++;
			i = 11;
			break;
		}

	}

}
